<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kgc.mapper.Leader_messMapper">
    <select id="getAllLeaderMess" resultType="com.kgc.pojo.leader.Leader_mess">
        select * from Leader_mess where messTypeId=#{messTypeId}
    </select>
    <update id="add" parameterType="com.kgc.pojo.leader.Leader_mess">
        insert into Leader_mess values(null,#{mess},#{messTypeId})
    </update>

</mapper>